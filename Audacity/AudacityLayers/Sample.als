module Sample


////////////////////////////////////////////////////////////////////////////////////////////
//                                             Signatures                                                //
////////////////////////////////////////////////////////////////////////////////////////////

sig Sample {}

sig ID {}

abstract sig Container {
	_id : ID
} 

fact {
	~_id in ID -> lone Container 	
}


////////////////////////////////////////////////////////////////////////////////////////////
//                                             Predicates                                                //
////////////////////////////////////////////////////////////////////////////////////////////

pred Equals[cont0, cont1 : Container] {
	cont0._id = cont1._id
}
